{
  "name": "widy-frontend",
  "version": "0.1.16",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "sb": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint './src/**/*.{js,jsx}' --fix --max-warnings=0",
    "lint:css": "stylelint './src/**/*.{js,jsx}'",
    "format": "prettier --write",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "is-hotkey": "^0.1.4",
    "jest-styled-components": "^6.3.1",
    "lodash.noop": "^3.0.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-beautiful-dnd": "^11.0.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.2",
    "react-joyride": "^2.0.5",
    "react-notification-system": "^0.2.17",
    "react-notification-system-redux": "^2.0.0",
    "react-redux": "^6.0.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-sizeme": "^2.6.7",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.1",
    "reselect": "^4.0.0",
    "slate": "^0.45.1",
    "slate-react": "^0.21.24",
    "storybook-readme": "^4.0.5",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@storybook/addon-actions": "^5.1.0-beta.0",
    "@storybook/addon-knobs": "^5.1.0-beta.0",
    "@storybook/addon-links": "^5.1.0-beta.0",
    "@storybook/addon-options": "^5.1.0-beta.0",
    "@storybook/addons": "^5.1.0-beta.0",
    "@storybook/react": "^5.1.0-beta.0",
    "@types/prop-types": "^15.7.1",
    "babel-plugin-macros": "^2.5.1",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.6",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "storybook-react-router": "^1.0.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.7.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.{js,jsx}"
    ]
  }
}
